{
  "expo": {
    "name": "Perpustakaan Uvers",
    "slug": "perpusuversexpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/PublicAsset/appsIcon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photo to let you share them with your friends"
        }
      ]
    ],
    "assetBundlePatterns": [
      "*/"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.erwin128.perpusuversexpo"
    },
    "android": {
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/PublicAsset/appsIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.erwin128.perpusuversexpo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3e3472e5-864c-4699-b2ec-5cb61ad790bd"
      }
    },
    "owner": "erwin128"
  }
}
